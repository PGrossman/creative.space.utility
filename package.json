{
  "name": "creative-space-utility",
  "version": "1.0.0",
  "description": "Utility Calculator for creative.space",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "npm run clean && vite build && electron-builder",
    "clean": "rimraf dist-electron release dist",
    "electron": "electron .",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.28.0",
    "vitest": "^1.0.0",
    "wait-on": "^7.0.0",
    "zod": "^3.22.0"
  },
  "build": {
    "appId": "com.creativespace.utility",
    "productName": "creative.space.utility",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist-electron/**/*",
      "datasources/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "src/shared",
        "to": "shared"
      }
    ],
    "mac": {
      "target": "dmg",
      "icon": "/Volumes/ATOM RAID/Dropbox/_Personal Files/12 - AI Vibe Coding/02 - Cursor Projects/01 - creative.space.utility/datasources/cs-utility.icns"
    }
  },
  "dependencies": {
    "@types/papaparse": "^5.3.16",
    "papaparse": "^5.5.3"
  }
}
